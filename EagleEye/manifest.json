{
    "manifest_version": 3,
    "name": "PDLA EAGLE EYE",
    "description": "This extension Checks the Images and Make them visible",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "img_off.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*",
        "https://ajax.googleapis.com/",
        "history",
        "storage",
        "webNavigation"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://*/*", "file:///*"],
            "js": ["dom.js"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": ["jquery.js"]
}
